{
  "name": "doboard",
  "version": "0.0.1",
  "description": "Homemade todo app.",
  "private": true,
  "scripts": {
    "client:build": "rollup -c client/rollup.config.js",
    "client:watch": "rollup -c client/rollup.config.js -w",
    "client:test": "echo \"Error: no test specified\" && exit 1",
    "server:build": "rollup -c server/rollup.config.js",
    "server:watch": "rollup -c server/rollup.config.js -w",
    "server:dev": "npm-run-all --parallel server:start server:watch",
    "server:start": "node dist/server/index.js",
    "server:test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "paulin-sh",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-typescript": "^6.0.0",
    "@svgr/rollup": "^5.4.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "husky": "^5.0.0-beta.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "rollup": "^2.28.2",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-terser": "^7.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "core-js-pure": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.7",
    "tslib": "^2.0.1"
  },
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
